  inputs 0
  name BackdropNode4
  tile_color 0x464646ff
  label "Used for\nmatrix knob"
  note_font_size 20
  xpos 143
  ypos 497
  bdwidth 295
  bdheight 621
 }
 Input {
  inputs 0
  name Input
  xpos 581
  ypos 593
 }
